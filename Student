public class Student {
	private String name, major, netId;
	private int uin;
	private double gpa;

	public Student(String name, String major, String netId, int uin, double gpa) {
		super();
		this.name = name;
		this.major = major;
		this.netId = netId;
		this.uin = uin;
		this.gpa = gpa;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getMajor() {
		return major;
	}

	public void setMajor(String major) {
		this.major = major;
	}

	public String getNetID() {
		return netId;
	}

	public void setNetID(String netID) {
		this.netId = netID;
	}

	public int getUin() {
		return uin;
	}

	public void setUin(int uin) {
		this.uin = uin;
	}

	public double getGpa() {
		return gpa;
	}

	public void setGpa(double gpa) {
		this.gpa = gpa;
	}

	@Override
	public String toString() {
		return "Student [name =" + name + ", Major" + major + ", netId" + netId + ", uin" + uin + ", gpa" + gpa;
	}

}
